# Audio
/persist/audio(/.*)?													u:object_r:persist_audio_file:s0

# Data files
/data/vendor/thermal(/.*)?												u:object_r:thermal_data_file:s0
/data/vendor/mac_addr(/.*)?												u:object_r:wifi_vendor_data_file:s0

# Camera
/(vendor|system/vendor)/bin/remosaic_daemon                								u:object_r:remosaic_daemon_exec:s0
/mnt/vendor/persist/camera(/.*)?           										u:object_r:persist_camera_file:s0
/dev/drv8846_dev                              										u:object_r:camera_motor_device:s0

# Fingerprint devices
/dev/goodix_fp                                										u:object_r:fingerprint_device:s0

# HALs
/(vendor|system/vendor)/bin/hw/android\.hardware\.biometrics\.fingerprint@2\.1-service\.xiaomi_davinci			u:object_r:hal_fingerprint_default_exec:s0
/(vendor|system/vendor)/bin/hw/android\.hardware\.light@2\.0-service\.xiaomi_sm6150					u:object_r:hal_light_default_exec:s0
/(vendor|system/vendor)/bin/hw/vendor\.xiaomi\.hardware\.motor@1.0-service						u:object_r:hal_lineage_camera_motor_default_exec:s0
/(vendor|system/vendor)/bin/hw/vendor\.lineage\.biometrics\.fingerprint\.inscreen@1.0-service\.xiaomi_davinci		u:object_r:hal_lineage_fod_default_exec:s0
/(vendor|system/vendor)/bin/hw/vendor\.lineage\.livedisplay@2\.0-service\.xiaomi_sm6150					u:object_r:hal_lineage_livedisplay_qti_exec:s0
/(vendor|system/vendor)/bin/hw/android\.hardware\.power@1\.3-service\.sm6150-libperfmgr					u:object_r:hal_power_default_exec:s0


# DSP
/dsp(/.*)?														u:object_r:adsprpcd_file:s0

# Graphics
/sys/devices/platform/soc/[a-z0-9]+.qcom,mdss_mdp/drm/card([0-3])+/card([0-3])+-DSI-1/modes		u:object_r:sysfs_graphics:s0
/sys/devices/platform/soc/[a-z0-9]+.qcom,mdss_mdp/drm/card([0-3])+/card([0-3])+-DSI-1/status		u:object_r:sysfs_graphics:s0
/sys/devices/platform/soc/soc:qcom,cam-req-mgr/video4linux/video[0-9]+/name(/.*)?                       u:object_r:sysfs_graphics:s0

# LED
/sys/devices/platform/soc/[a-z0-9]+.qcom,spmi/spmi-[0-1]/spmi0-0[0-9]/[a-z0-9]+.qcom,spmi:qcom,[a-z0-9]+@[0-9]:qcom,leds@d000/leds/white(/.*)?	u:object_r:sysfs_leds:s0

# Persist
 /persist(/.*)?														u:object_r:persist_file:s0

# SSR
/sys/devices(/platform)?/soc/[a-z0-9\.:]+,[a-z0-9\-\_]+/subsys[0-9]+/name						u:object_r:sysfs_ssr:s0

# Mlipay
/vendor/bin/mlipayd@1\.1												u:object_r:hal_mlipay_default_exec:s0

# Thermal
/(vendor|system/vendor)/bin/mi_thermald											u:object_r:mi_thermald_exec:s0

# WiFi
/vendor/bin/nv_mac													u:object_r:wcnss_service_exec:s0
/vendor/bin/wifi-mac-generator												u:object_r:wcnss_service_exec:s0
